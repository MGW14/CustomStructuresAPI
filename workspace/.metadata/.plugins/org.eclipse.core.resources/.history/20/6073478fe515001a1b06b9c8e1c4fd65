package work.mgnet.customstructuresapi.manager;

import java.util.ArrayList;

import org.bukkit.Material;
import org.bukkit.inventory.ItemStack;

public class Structure {
	
	public String name;
	public ArrayList<Material> blocks;
	public ArrayList<ItemStack> commonLoot;
	public ArrayList<ItemStack> uncommonLoot;
	public ArrayList<ItemStack> rareLoot;
	public ArrayList<ItemStack> epicLoot;
	public ArrayList<ItemStack> legendaryLoot;
	
	public Structure(String name, ArrayList<Material> blocks, ArrayList<ItemStack> commonLoot,
			ArrayList<ItemStack> uncommonLoot, ArrayList<ItemStack> rareLoot, ArrayList<ItemStack> epicLoot,
			ArrayList<ItemStack> legendaryLoot) {
		super();
		this.name = name;
		this.blocks = blocks;
		this.commonLoot = commonLoot;
		this.uncommonLoot = uncommonLoot;
		this.rareLoot = rareLoot;
		this.epicLoot = epicLoot;
		this.legendaryLoot = legendaryLoot;
	}
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public ArrayList<Material> getBlocks() {
		return blocks;
	}
	public void setBlocks(ArrayList<Material> blocks) {
		this.blocks = blocks;
	}
	public ArrayList<ItemStack> getCommonLoot() {
		return commonLoot;
	}
	public void setCommonLoot(ArrayList<ItemStack> commonLoot) {
		this.commonLoot = commonLoot;
	}
	public ArrayList<ItemStack> getUncommonLoot() {
		return uncommonLoot;
	}
	public void setUncommonLoot(ArrayList<ItemStack> uncommonLoot) {
		this.uncommonLoot = uncommonLoot;
	}
	public ArrayList<ItemStack> getRareLoot() {
		return rareLoot;
	}
	public void setRareLoot(ArrayList<ItemStack> rareLoot) {
		this.rareLoot = rareLoot;
	}
	public ArrayList<ItemStack> getEpicLoot() {
		return epicLoot;
	}
	public void setEpicLoot(ArrayList<ItemStack> epicLoot) {
		this.epicLoot = epicLoot;
	}
	public ArrayList<ItemStack> getLegendaryLoot() {
		return legendaryLoot;
	}
	public void setLegendaryLoot(ArrayList<ItemStack> legendaryLoot) {
		this.legendaryLoot = legendaryLoot;
	}
	
}
